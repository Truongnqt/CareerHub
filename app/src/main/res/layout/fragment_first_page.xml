<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.nguyenquoctruongshome.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>

    <LinearLayout
        android:id="@+id/linearSplashScreen"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageLohoappnameOne"
            android:layout_width="@dimen/_252pxh"
            android:layout_height="@dimen/_63pxh"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/_300pxv"
            android:layout_marginBottom="@dimen/_5pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_loho_appname"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_loho_appname" />
    </LinearLayout>
</layout>
